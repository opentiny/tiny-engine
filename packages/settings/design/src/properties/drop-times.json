{
  "schema": {
    "type": "object",
    "properties": {
      "end": {
        "title": "结束时间，以分钟计算",
        "type": "number",
        "default": 1440
      },
      "placeholder": {
        "title": "设置输入框内的提示占位文本",
        "type": "string",
        "default": ""
      },
      "popper-append-to-body": {
        "title": "是否将弹出框插入至 body 元素。在弹出框的定位出现问题时，可将该属性设置为 false (可参考 select 组件)",
        "type": "boolean",
        "default": true
      },
      "$$popper-class": {
        "title": "为 popper 添加类名(可参考 popover 组件)",
        "type": "string",
        "default": ""
      },
      "size": {
        "title": "设置组件大小",
        "type": "string",
        "enum": ["medium", "small", "mini"],
        "enumNames": ["中等尺寸", "较小尺寸", "迷你尺寸"],
        "default": ""
      },
      "start": {
        "title": "开始时间，以分钟计算",
        "type": "number",
        "default": 0
      },
      "step": {
        "title": "间隔时间，以分钟计算",
        "type": "number",
        "default": 15
      }
    }
  }
}
